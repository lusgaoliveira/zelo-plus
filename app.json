{
  "expo": {
    "name": "zelo",
    "slug": "zelo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "newArchEnabled": true,
    "scheme": "zelo",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FEF6E4"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lusga.zelo"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#FEF6E4"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.lusga.zelo",
      "permissions": ["NOTIFICATIONS"]  
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "05d8be47-5916-447f-b572-8c4ffdf9c8d7"
      }
    },
    "owner": "lusga"
  }
}
